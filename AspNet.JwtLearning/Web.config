<?xml version="1.0" encoding="utf-8"?>

<configuration>
	<configSections>
		<!--log4net节点添加-->
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>

	<appSettings>	
		<add key="AllowOrigins" value="http://localhost:8080" />
		<add key="AuthHeaderName" value="Authorization" />
	</appSettings>

	<connectionStrings>
		<!--mysql 线上-->
		<add name="dbauthtest" providerName="MySql.Data.MySqlClient" connectionString="server=139.198.122.83;port=3300; database=dbauthtest;User ID=zx1;Password=zx@123456" />
		<!--redis 线上-->
		<add name="redisfortest" connectionString="139.198.122.83:6300,password=zx@123456,allowadmin=true" />
		<!--serversql express版-->
		<add name="SqlServerTestDB" providerName="System.Data.SqlClient" connectionString="Data Source=(LOCAL)\SQLEXPRESS;User Id=sa; Password=1996crimdeath; Initial Catalog=SqlServerTestDB;Persist Security Info=no;Integrated Security=no;" />
	</connectionStrings>

	<!--log4net 配置-->
	<log4net>
		<logger name="LogInfo">
			<level value="ALL" />
			<appender-ref ref="FileAppender" />
		</logger>
		<appender name="FileAppender" type="AspNet.JwtLearning.App_Start.MyFileAppender">
			<file value="Log/" />
			<!--生成的文件夹在Log文件夹下(相对路径) -->
			<preserveLogFileNameExtension value="true" />
			<maxSizeRollBackups value="10" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datePattern value="&quot;Logs_&quot;yyyyMMdd&quot;.txt&quot;" />
			<staticLogFileName value="false" />

			<layout type="log4net.Layout.PatternLayout,log4net">
				<param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
			</layout>
		</appender>
	</log4net>

	<system.web>
		<compilation debug="true" targetFramework="4.7.2" />
		<httpRuntime targetFramework="4.7.2" />
	</system.web>

	<system.webServer>
		<handlers>
			<remove name="ExtensionlessUrlHandler-Integrated-4.0" />
			<remove name="OPTIONSVerbHandler" />
			<remove name="TRACEVerbHandler" />
			<add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
		</handlers>
	</system.webServer>

	<runtime>
